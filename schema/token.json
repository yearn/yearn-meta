{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "token.json",
  "title": "Token",
  "description": "An underlying token of a Yearn vault",
  "type": "object",
  "properties": {
    "description": {
      "type": "string"
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "website": {
      "type": "string"
    },
    "tokenIconOverride": {
      "type": "string"
    },
    "tokenSymbolOverride": {
      "type": "string"
    },
    "tokenNameOverride": {
      "type": "string"
    },
    "supportedZaps": {
      "type": "object",
      "properties": {
        "ftmApeZap": {
          "type": "boolean"
        }
      }
    }
  },
  "required": []
}
